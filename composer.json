{
    "name": "backpack/crud",
    "version": "3.2.27",
    "description": "Quickly build an admin interface for your Eloquent models, using Laravel 5. Build a CMS in a matter of minutes.",
    "keywords": [
        "CRUD",
        "create",
        "update",
        "delete",
        "read",
        "admin panel",
        "admin interface",
        "CMS"
    ],
    "homepage": "https://github.com/laravel-backpack/CRUD",
    "license": "MIT",
    "authors": [
        {
            "name": "Cristian tabacitu",
            "email": "hello@tabacitu.ro",
            "homepage": "http://www.tabacitu.ro",
            "role": "Chief Architect & Lead Developer"
        }
    ],
    "require": {
        "backpack/base": "dev-main",
        "laravelcollective/html": "~5.0|^6.0",
        "livecontrol/eloquent-datatable": "dev-main",
        "doctrine/dbal": "^2.5",
        "venturecraft/revisionable": "1.*",
        "intervention/image": "^2.3"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "orchestra/testbench": "~3.0",
        "orchestra/database": "~3.4"
    },
    "autoload": {
        "psr-4": {
            "Backpack\\CRUD\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Backpack\\CRUD\\Tests\\": "tests"
        },
        "exclude-from-classmap": [
            "**/config"
        ]
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Backpack\\CRUD\\CrudServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/Meer-Development-The-Dev-Squad/Backpack-Base.git"
        },
        {
            "type": "git",
            "url": "https://github.com/Meer-Development-The-Dev-Squad/EloquentDataTable.git"
        }
    ]
}
